source ~/.history
source ~/.terminal_colors

export ARCHFLAGS="-arch x86_64"

export EDITOR=vim
export JEWELER_OPTS="--rspec"

source ~/.os_specific/paths

if [ "$TERM" != "dumb" ]; then
    [ -e "$HOME/.dir_colors" ] && 
    DIR_COLORS="$HOME/.dir_colors" [ -e "$DIR_COLORS" ] || DIR_COLORS="" 
    eval "`dircolors -b $DIR_COLORS`" 
    alias ls='ls --color=auto'
fi

